{
  "name": "fruit20-15",
  "version": "0.1.0",
  "author": "David Owen",
  "description": "Angular 2.0/1.5 Hybrid Reference App (ng-rosetta series)",
  "license": "BSD3",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --display-error-details --port 9215",
    "test": "karma start config/karma.conf.js",
    "copyresources": "cpx \"src/bower_components/**\" dist/assets/bower_components && cpx \"node_modules/core-js/client/shim.min.js\" dist/assets && cpx \"node_modules/zone.js/dist/zone.min.js\" dist/assets",
    "postinstall": "bower install && typings install && npm run copyresources",
    "clean:aot": "rimraf aot",
    "build:aot1": "ngc -p tsconfig-aot.1.json",
    "build:aot2": "ngc -p tsconfig-aot.2.json",
    "build:aot3": "rollup -c rollup-config.js",
    "build:aot4": "tsc -p tsconfig-es5.json",
    "serve:aot": "lite-server -c config/lite-server-config.json",
    "build:aot": "echo \"Clean the AOT directory.\" && npm run clean:aot && echo \"Run the AOT 1st pass.\" && npm run build:aot1 && echo \"Run the AOT 2nd pass.\" && npm run build:aot2 && echo \"Run the RollupJS task.\" && npm run build:aot3 && echo \"Run the ES5 Transpiler Task.\" && npm run build:aot4 && echo \"Launch the server.\" && npm run serve:aot"
  },
  "dependencies": {
    "@angular/common": "2.2.4",
    "@angular/compiler": "2.2.4",
    "@angular/compiler-cli": "2.2.4",
    "@angular/core": "2.2.4",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.2.4",
    "@angular/platform-browser": "2.2.4",
    "@angular/platform-browser-dynamic": "2.2.4",
    "@angular/platform-server": "^2.2.4",
    "@angular/router": "3.2.4",
    "@angular/upgrade": "2.2.4",
    "angular": "1.5.7",
    "angular-translate": "^2.14.0",
    "angular-translate-loader-static-files": "^2.14.0",
    "angular-ui-router": "^0.3.2",
    "core-js": "^2.4.0",
    "reflect-metadata": "0.1.2",
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lite-server": "2.2.2",
    "ngtemplate-loader": "^1.3.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "rollup": "^0.37.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-loader": "^0.8.1",
    "typescript": "2.0.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.2",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-dev-server": "^1.15.1",
    "webpack-merge": "^0.14.0",
    "webpack-node-externals": "^1.5.4"
  },
  "main": "index.js"
}
